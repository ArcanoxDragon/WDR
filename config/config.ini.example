#Emoji servers to check First
EMOJI_SERVERS = "408997776672948224,504352437977743360,610630635437948929"

# Name the bot for embed dialogs
BOT_NAME = WDR

# Enable or Disable UIV scanning mode functionality
UIV = ENABLED

# Enable or Disable Console Logs
VERBOSE_LOGS = ENABLED

# Time mins remain for encounter to not post
TIME_REMAIN = 5
TIME_REMAIN_SUBS = 1

#CONTROL WHAT YOUR USERS SUBSCRIBE TO LEAVE EMPTY IF NO
BLACKLISTED = 50,263

# CHECK FOR DONORS
Donor_Check = ENABLED
donor_info = Please visit, http://url for more info
log_channel = channel_id
log_ping = optional role etc. remove if you don't want to use.

# LISTENING PORT
# You will need to add http://bot_host_ip:3333/ in RDM. You will need to port forward on your router if RDM does not reside in your local network.
LISTENING_PORT = 3333

# MAP TILES
# ENABLED or DISABLED. Includes Map Tile images in the embeds for feeds and subscriptions.
Map_Tiles = ENABLED
Tile_Width = 400
Tile_Height = 220

## IMAGE HOST
# This is required for your map tiles to work.
# This must be URL. You must include http:// or https:// and the / at the end.
HOST = http://www.yourimages.com/

## IMAGE DIRECTORY
# pokemon_tiles, raid_tiles, and quest_tiles folders must be created in this location.
# This is where your map tiles are saved/cached for bot to reference from IMAGE HOST URL
# You will need to edit the .conf files based on your setup. Example conf is located in /files/nginx of what is needed.
# You will need to know and implement correct permissions for nginx.
IMAGE_DIR = ./static/images/

## Self Hosted or Other Tile Server Options. These are optional, remove lines or ## out.
Tile_Server = https://optional.url/{z}/{x}/{y}.png
Tile_Marker = https://optional.url
Spawn_Marker = https://optionspawn.png
Gym_Marker = https://optiongym.png
Stop_Marker = https://optionstop.png

#Optional URL for your front end for [Scan Map] embed link
FRONTEND_URL = https://yourmap.com/

# SPRITES TYPE DEFAULT / SHUFFLE comment or un-comment the base url
SPRITE_TYPE = DEFAULT
SPRITE_URL = https://www.serebii.net/sunmoon/pokemon/
#SPRITE_URL = https://raw.githubusercontent.com/nileplumb/PkmnShuffleMap/master/PMSF_icons_large/

# YOUR TIMEZONE
TIMEZONE = America/New_York

# WEBHOOK COORDINATE TIMEZONE ENABLED/DISABLED (IF NOT ENABLED TIMEZONE WILL DEFAULT TO "SERVER" TIMEZONE)
coordinate_timezone = DISABLED

# COMMAND PREFIX
PREFIX = '.'

# AUTO-DELETE MESSAGES IN COMMAND CHANNEL
Tidy_Channel = ENABLED

# Raid Lobbies that are created upon user reaction to Raid Embeds
Raid_Lobbies = ENABLED
# The number of raid lobbies people can "create" within a 15 minute period
Lobby_Limit = 2

